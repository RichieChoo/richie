{
  "name": "@richie/npm-alias",
  "version": "0.2.0",
  "description": "provide frequently used npm alias ",
  "keywords": [
    "npm",
    "alias"
  ],
  "bin": {
    "ni": "npm install",
    "nu": "npm update",
    "nup": "npm run update",
    "nd": "npm run dev",
    "ns": "npm run serve",
    "nus": "rm -rf node_modules/.cache && npm install && npm update && npm run serve",
    "nub": "rm -rf node_modules/.cache && npm install && npm update && npm run build"
  },
  "author": "RichieChoo <richiechoo1993@gmail.com>",
  "homepage": "https://github.com/RichieChoo/richie#readme",
  "license": "MIT",
  "main": "lib/npm-alias.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RichieChoo/richie.git"
  },
